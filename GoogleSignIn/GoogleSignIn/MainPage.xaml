<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="GoogleSignIn.MainPage">

    <StackLayout>
        <Button Text="login with Goolge"
                Clicked="Button_Clicked" />
        <Button Text="login with Spotify"
                Clicked="Button_Clicked_2" />
        <Button Text="login with Facebook"
                Clicked="Button_Clicked_1" />
        <Button Text="login with Tesla"
                Clicked="Button_Clicked_3" />

        <WebView x:Name="TeslaView"
                 WidthRequest="500"
                 HeightRequest="500"
                 IsVisible="false"></WebView>
    </StackLayout>

</ContentPage>
